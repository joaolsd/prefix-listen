#!/bin/sh

HTTP_PORT=8080
HTTPS_PORT=8443
CERT_FILE=/usr/local/etc/nginx/letsencrypt/wild-@cc@e.dotnxdomain.net.crt
KEY_FILE=/usr/local/etc/nginx/letsencrypt/wild-@cc@e.dotnxdomain.net.key

BINARY=/usr/local/bin/web-1x1

ulimit -c unlimited
exec ${BINARY} -c ${CERT_FILE} -k ${KEY_FILE} -p ${HTTP_PORT} -s ${HTTPS_PORT} > /var/log/web-1x1.log
